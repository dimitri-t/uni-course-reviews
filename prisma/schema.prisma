// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema


generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Courses {
  courseCode String @unique
  uniOrg String
  title String
  description String
  level String
  avgDifficulty String
  avgRating String
}